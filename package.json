{
  "name": "cms-project",
  "version": "1.0.0",
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --config eslint.config.cjs --fix",
      "prettier --write"
    ]
  },
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "type": "commonjs",
  "scripts": {
    "prepare": "husky",
    "dev": "dotenv -e .env.development -- cross-env NODE_ENV=development nodemon --watch src --ext ts,js --exec \"ts-node src/server.ts\"",
    "build": "tsc",
    "start": "dotenv -e .env.production -- cross-env NODE_ENV=production node dist/server.js",
    "prod": "npm run start",
    "start:test": "dotenv -e .env.test -- cross-env NODE_ENV=test node dist/server.js",
    "test": "npm run start:test",
    "test:ci": "dotenv -e .env.test -- cross-env NODE_ENV=test jest",
    "test:cover": "dotenv -e .env.test -- cross-env NODE_ENV=test jest --coverage --coverageThreshold='{ \"global\": { \"branches\": 80, \"functions\": 80, \"lines\": 80, \"statements\": 80 } }'",
    "docker:dev": "docker compose --env-file .env.development up -d",
    "docker:test": "docker compose --env-file .env.test up -d",
    "docker:prod": "docker compose --env-file .env.production up -d",
    "docker:stop:dev": "docker compose --env-file .env.development down",
    "docker:stop:test": "docker compose --env-file .env.test down",
    "docker:stop:prod": "docker compose --env-file .env.production down",
    "db:setup:dev": "dotenv -e .env.development -- cross-env NODE_ENV=development ts-node scripts/db-setup.ts",
    "db:setup:test": "dotenv -e .env.test -- cross-env NODE_ENV=test ts-node scripts/db-setup.ts",
    "db:setup:prod": "dotenv -e .env.production -- cross-env NODE_ENV=production ts-node scripts/db-setup.ts",
    "migration:create": "mikro-orm migration:create",
    "migration:up:dev": "dotenv -e .env.development -- cross-env NODE_ENV=development mikro-orm migration:up",
    "migration:down:dev": "dotenv -e .env.development -- cross-env NODE_ENV=development mikro-orm migration:down",
    "migration:status:dev": "dotenv -e .env.development -- cross-env NODE_ENV=development mikro-orm migration:status",
    "migration:up:test": "dotenv -e .env.test -- cross-env NODE_ENV=test mikro-orm migration:up",
    "migration:down:test": "dotenv -e .env.test -- cross-env NODE_ENV=test mikro-orm migration:down",
    "migration:status:test": "dotenv -e .env.test -- cross-env NODE_ENV=test mikro-orm migration:status",
    "migration:up:prod": "dotenv -e .env.production -- cross-env NODE_ENV=production mikro-orm migration:up",
    "migration:down:prod": "dotenv -e .env.production -- cross-env NODE_ENV=production mikro-orm migration:down",
    "migration:status:prod": "dotenv -e .env.production -- cross-env NODE_ENV=production mikro-orm migration:status",
    "lint": "eslint --config eslint.config.cjs \"src/**/*.{ts,js}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "import:tables": "ts-node src/cli/schema-import.ts tables",
    "import:columns": "ts-node src/cli/schema-import.ts columns",
    "orm:check": "mikro-orm migration:check",
    "benchmark": "artillery quick --count 10 http://localhost:3000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "9.7.0",
    "@mikro-orm/cli": "6.4.16",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cross-env": "^7.0.3",
    "cypress": "13.13.0",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^9.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "7.37.5",
    "glob": "^11.0.3",
    "globals": "16.3.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "^16.1.2",
    "mongodb": "^6.18.0",
    "mysql2": "^3.14.2",
    "nodemon": "^3.0.1",
    "prettier": "3.3.2",
    "supertest": "7.0.0",
    "ts-jest": "29.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@getbrevo/brevo": "2.5.0",
    "@mikro-orm/core": "6.4.16",
    "@mikro-orm/migrations": "6.4.16",
    "@mikro-orm/mongodb": "6.4.16",
    "@mikro-orm/mysql": "6.4.16",
    "@mikro-orm/seeder": "6.4.16",
    "@sendgrid/mail": "8.1.5",
    "bcrypt": "6.0.0",
    "bullmq": "5.56.5",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.2.5",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "mailersend": "2.6.0",
    "mailgun-js": "0.22.0",
    "node-mailjet": "6.0.8",
    "nodemailer": "7.0.5",
    "passport": "^0.7.0",
    "pm2": "6.0.8",
    "react": "19.1.0",
    "redis": "^5.6.0",
    "redux": "5.0.1",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "tailwindcss": "4.1.11",
    "uuid": "11.1.0",
    "validator": "13.15.15",
    "winston": "3.17.0",
    "xlsx": "^0.18.5"
  }
}
